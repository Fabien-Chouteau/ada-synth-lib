project Ada_Audio is
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("audio.adb");

   package Builder is
      for Global_Configuration_Pragmas use "./gnat.adc";
   end Builder;

   package Compiler is
      for Default_Switches ("Ada") use ("-O0", "-g");
   end Compiler;
end Ada_Audio;
